<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="codelabs.m.appfoodscontact">

    <application
            android:name=".AppFoodsContactApplication"
            android:allowBackup="true"
            android:icon="@drawable/ic_foodcontact"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui_sqlite.LoginSqliteActivity">
        </activity>
        <activity android:name=".ui_sqlite.RegisterSqliteActivity">
        </activity>
        <activity android:name=".ui_firebase.LoginFbzActivity">
        </activity>
        <activity android:name=".ui_firebase.RegisterFbzActivity">
        </activity>
        <activity
                android:name=".HomeActivity"
                android:label="@string/title_activity_home"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".ui_firebase.HomeActivityFbz"
                android:label="@string/title_activity_home"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>